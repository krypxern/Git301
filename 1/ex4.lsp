(define fib (m)
	(if (= m 0)
		0
		(if (= m 1)
			1
			(+ (fib (- m 1)) (fib (- m 2))))))

(print 9999)
(print (= (fib 0) 0))
(print (= (fib 1) 1))
(print (= (fib 2) 1))
(print (= (fib 3) 2))
(print (= (fib 4) 3))
(print (= (fib 5) 5))
